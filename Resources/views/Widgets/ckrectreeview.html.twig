{% if node.node.deleted ==false and node.nodetranslation.online %}
<li class="{% if node.active %}jstree-open{% endif %} {% if nodemenu.current is not null and nodemenu.current.id == node.id  %}active{% endif %}" rel="page">
    <a onclick="jQuery('#selection').text('Selection: {{ path("_slug", { "url": node.slug }) }}'); ckurl = '{{ path("_slug", { "url": node.slug }) }}';" class="item" href="#">{{ node.title }}</a>
    <ul>
        {% for node in node.children %}
            {% include 'KunstmaanNodeBundle:Widgets:ckrectreeview.html.twig' %}
        {% endfor %}
    </ul>
</li>
{% endif %}
